{ "$schema": 
    "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json", 
    "logo": {
        "type": "small",
        "color":{
            "1" : "yellow",
            "2" : "magenta",
        },
        "padding": {
            "right": 1,
        }
    },
    "display": {
        "separator": " > ",
