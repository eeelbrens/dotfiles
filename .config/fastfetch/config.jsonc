            "symbol": "diamond",
            "key": " clrs",
        },
        {
            "type": "kernel",
            "key": " krnl"
        },
        {
            "type": "uptime",
            "key": " uptm"
        },
        {
            "type": "wm",
            "key": "   wm"
        },
        {
            "type": "shell",
            "key": " shll"
        },
        {
            "type": "packages",
            "key": " pkgs"
        },
    ]
}
